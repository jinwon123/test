package p01.object_class_equals;

// 1. Object class의 equals method 동작 방식
// - ' == ' 연산자(operator)와 같은 결과를 도출
// - obj1 == obj2의 결과가 false가 나와서 obj1.equals(obj2)의 결과도 false가 나온다.
public class ObjectEqualsEx {

	public static void main(String[] args) {
		Object obj1 = new Object();
		Object obj2 = new Object();
		String name1 = new String("홍길동");
		String name2 = new String("홍길동");
		int x = 10;
		int y = 10;
		
		// obj1, obj2의 힙메모리 시작 주소값 구하기
		System.out.println(System.identityHashCode(obj1));
		System.out.println(System.identityHashCode(obj2));
		
		boolean result = obj1.equals(obj2);
		System.out.println("result : " + result);
		
		result = (obj1 == obj2); // heap memory의 주소값 비교
		System.out.println("result : " + result);
		
		result = (x == y);		// 값을 비교
		System.out.println("result : " + result);
		
		result = (name1 == name2); // heap memory의 주소값 비교
		System.out.println("result : " + result);
		
		result = name1.equals(name2);	// override
		System.out.println("result : " + result);

	}

}
