package backJun_for;

import java.util.Scanner;

public class Backjun8 {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int a = sc.nextInt();
		int b = 0;
		int c = 0;
		int[] sum = new int [a];
		
		for (int i=0; i<sum.length; i++) {
			b = sc.nextInt();
			c = sc.nextInt();
			sum[i] = b + c;
		}
		
		for (int i=0; i<sum.length; i++) {
			System.out.println("Case #" + i + ": " + b + " + " + c + " = " + sum[i]);
		}

	}

}
