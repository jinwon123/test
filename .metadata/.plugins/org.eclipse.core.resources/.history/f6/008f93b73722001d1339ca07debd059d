package backJun_for;

import java.util.Scanner;

public class Backjun11 {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		
		int N = sc.nextInt();
		int X = sc.nextInt();
		int[] a = new int[N];
		int[] sum = new int[N];

		if (1<=N && N<= 10000) {
			if (1<=X && X<= 10000) {
				for (int i = 0; i<N; i++) {
					a[i] = sc.nextInt();
					if (a[i]<X) {
						System.out.print(a[i]);
				}
					
			}
		}
		}
	}
}
